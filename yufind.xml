<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Yale Library - Yufind</os:ShortName>
<os:Description>Search Yale's library catalog with the Yufind interface</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>https://raw.github.com/triplingual/yale-opensearch-extensions/master/yufind.xml</UpdateUrl>
<IconUpdateUrl>http://tripkirkpatrick.commons.yale.edu/files/2012/07/classic_y.png</IconUpdateUrl>
<SearchForm>http://yufind.library.yale.edu</SearchForm>
<os:Url type="text/html" method="GET" template="http://yufind.library.yale.edu/yufind/Search/Results?lookfor={searchTerms}&amp;sourceid=mozilla-search">
</os:Url><os:Url type="application/opensearchdescription+xml" method="GET" template="https://raw.github.com/triplingual/yale-opensearch-extensions/master/yufind.xml" rel="self">
